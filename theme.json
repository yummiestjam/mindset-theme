{
	"customTemplates": [
		{
			"name": "wp-custom-template-full-width-page",
			"postTypes": [
				"page"
			],
			"title": "Full Width Page"
		}
	],
	"settings": {
		"appearanceTools": true,
		"layout": {
			"contentSize": "620px",
			"wideSize": "1000px"
		},
		"spacing": {
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
					"name": "System Font",
					"slug": "system-font"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/button": {
				"border": {
					"bottom": {
						"color": "var(--wp--preset--color--luminous-vivid-amber)",
						"style": "solid",
						"width": "2px"
					},
					"left": {
						"color": "var(--wp--preset--color--luminous-vivid-amber)",
						"style": "solid",
						"width": "2px"
					},
					"right": {
						"color": "var(--wp--preset--color--luminous-vivid-amber)",
						"style": "solid",
						"width": "2px"
					},
					"top": {
						"color": "var(--wp--preset--color--luminous-vivid-amber)",
						"style": "solid",
						"width": "2px"
					}
				},
				"variations": {
					"outline": {
						"border": {
							"bottom": {
								"color": "var(--wp--preset--color--luminous-vivid-amber)",
								"style": "solid",
								"width": "2px"
							},
							"left": {
								"color": "var(--wp--preset--color--luminous-vivid-amber)",
								"style": "solid",
								"width": "2px"
							},
							"right": {
								"color": "var(--wp--preset--color--luminous-vivid-amber)",
								"style": "solid",
								"width": "2px"
							},
							"top": {
								"color": "var(--wp--preset--color--luminous-vivid-amber)",
								"style": "solid",
								"width": "2px"
							}
						},
						"color": {
							"background": "var(--wp--preset--color--black)",
							"text": "var(--wp--preset--color--white)"
						}
					}
				}
			},
			"core/pullquote": {
				"elements": {
					"cite": {
						"typography": {
							"fontSize": "var(--wp--preset--font-size--medium)"
						}
					}
				},
				"spacing": {
					"padding": {
						"bottom": "var(--wp--preset--spacing--50)",
						"top": "var(--wp--preset--spacing--50)"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			}
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header",
			"title": "Header"
		},
		{
			"area": "footer",
			"name": "footer",
			"title": "Footer"
		},
		{
			"area": "uncategorized",
			"name": "sidebar",
			"title": "Sidebar"
		}
	],
	"version": 3,
	"$schema": "https://schemas.wp.org/wp/6.8/theme.json"
}